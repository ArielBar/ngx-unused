{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"strict": true,
		"types": ["node"],
		"lib": ["ES2019"],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist"
	},
	"include": ["lib/**/*.ts"]
}
